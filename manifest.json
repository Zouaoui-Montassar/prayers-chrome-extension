{
    "manifest_version": 3,
    "name": "Prayers Extension",
    "description": "Prayers extension",
    "version": "1.0",
    "background": {
    "service_worker": "dist/background.js"
  },
  "permissions": [
    "notifications",
    "storage",
    "alarms"
  ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "assets/prayers_icon.png"
    },
    "icons": {
    "48": "assets/placeholder.png"
  }
  }